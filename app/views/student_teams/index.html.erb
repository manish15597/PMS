<p id="notice"><%= notice %></p>

<h1>Student Teams</h1>

<table>
  <thead>
    <tr>
      <th>Student</th>
      <th>Team</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @student_teams.each do |student_team| %>
      <tr>
        <td><%= student_team.student_id %></td>
        <td><%= student_team.team_id %></td>
        <td><%= link_to 'Show', student_team %></td>
        <td><%= link_to 'Edit', edit_student_team_path(student_team) %></td>
        <td><%= link_to 'Destroy', student_team, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

