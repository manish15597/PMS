<p id="notice"><%= notice %></p>

<h1>Submissions</h1>

<table>
  <thead>
  <tr>
    <th>Project</th>
    <th>Report and Files</th>
    <th>Feedback</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @ps.each do |submission| %>
      <tr>
        <td><%= submission.project_id %></td>
        <td><%= submission.report %></td>
        <td><%= submission.feedback %></td>
        <td><%= link_to 'Give Feedback', edit_submission_path(submission) if current_user.usertype == 1 %></td>

      </tr>
  <% end %>
  </tbody>
</table>

<br>

